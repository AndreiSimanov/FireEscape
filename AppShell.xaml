<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="FireEscape.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:FireEscape.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="{StaticResource AppTitle}">

    <ShellContent  Title="{StaticResource AppTitle}" ContentTemplate="{DataTemplate local:MainPage}" Route="MainPage" />
    <ShellContent Title="{StaticResource ProtocolPageTitle}" ContentTemplate="{DataTemplate local:ProtocolPage}" Route="ProtocolPage" />

</Shell>
